        "raw-loader": "^4.0.2",
        "reflect-metadata": "^0.1.13",
        "replace-in-file-webpack-plugin": "^1.0.6",
        "sass": "^1.64.2",
        "sass-loader": "^13.3.2",
        "scss-loader": "0.0.1",
        "style-loader": "^3.3.3",
        "svg-chunk-webpack-plugin": "^4.0.2",
        "svg-inline-loader": "^0.8.2",
        "svg-sprite-loader": "^6.0.11",
        "terser-webpack-plugin": "^5.3.9",
        "ts-loader": "^9.4.4",
        "typescript": "^4.9.5",
