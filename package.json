{
    "name": "webpack-angular",
    "version": "1.0.0",
    "description": "this is the project has a custom configuration in angular",
    "scripts": {
        "start": "cross-env NODE_ENV=dev webpack-dev-server --config config/webpack.dev.js --open --profile --progress",
        "build": "cross-env NODE_ENV=prod webpack --config config/webpack.prod.js --profile --progress",
        "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "test": "karma start",
        "test:sonar": "npm run lint && cross-env SONAR_QUBE=1 karma start",
        "test:ci": "karma start --single-run --browsers ChromeTravisCi"
    },
    "dependencies": {
        "@angular/common": "^16.0.0",
        "@angular/compiler": "^16.0.0",
        "@angular/core": "^16.0.0",
        "@angular/forms": "^16.0.0",
        "@angular/platform-browser": "^16.0.0",
        "@angular/platform-browser-dynamic": "^16.0.0",
        "@angular/router": "^16.0.0",
        "rxjs": "^7.8.1",
        "zone.js": "^0.13.1"
    },
    "devDependencies": {
        "@angular-devkit/build-optimizer": "^0.1302.1",
        "@angular/cli": "^16.0.0",
        "@angular/compiler-cli": "^16.0.0",
        "@angular/language-service": "^16.0.0",
        "@ngtools/webpack": "^16.2.0",
        "@types/core-js": "^2.5.5",
        "@types/hammerjs": "^2.0.41",
        "@types/node": "^20.4.9",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.8.1",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "extract-loader": "^5.1.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.3",
        "image-minimizer-webpack-plugin": "^3.8.3",
        "imagemin": "^8.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-mozjpeg": "^10.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-pngquant": "^9.0.2",
        "imagemin-svgo": "^10.0.1",
        "install": "^0.13.0",
        "karma": "^6.4.2",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-jasmine": "^5.1.0",
        "mini-css-extract-plugin": "^2.7.6",
        "node-sass": "^9.0.0",
        "npm": "^9.8.1",
